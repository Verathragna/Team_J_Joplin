    "tar": "^6.1.11",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@joplin/lib": "~2.9",
    "@codemirror/view": "6.22.2",
    "@codemirror/language": "6.10.0"
